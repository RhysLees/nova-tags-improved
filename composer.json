{
    "name": "rhyslees/nova-tags-improved",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/nova": "^4.22",
        "nova-kit/nova-packages-tool": "^v1.5.4",
        "rhyslees/nova-about": "^2.0"
    },
    "require-dev": {
        "laravel/pint": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Rhyslees\\NovaTagsImproved\\": "src/"
        }
    },
    "scripts": {
        "format": [
            "vendor/bin/pint"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rhyslees\\NovaTagsImproved\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
